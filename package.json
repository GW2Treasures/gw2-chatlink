{
  "name": "@gw2/chatlink",
  "version": "0.1.1",
  "description": "Encode and decode Guild Wars 2 chatlinks",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown --minify",
    "changeset": "changeset",
    "prerelease": "pnpm run build",
    "release": "changeset publish",
    "test": "vitest"
  },
  "keywords": [
    "gw2",
    "guildwars2",
    "guild wars 2",
    "chatlink",
    "chat",
    "chatcode",
    "link"
  ],
  "author": "darthmaim",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GW2Treasures/gw2-chatlink.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gw2treasures/gw2-chatlink/issues"
  },
  "homepage": "https://github.com/GW2Treasures/gw2-chatlink#readme",
  "packageManager": "pnpm@10.29.2",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@changeset-bot/changelog": "0.0.4",
    "@changesets/cli": "2.29.8",
    "tsdown": "0.20.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
